{
	"name": "promptfoo-runner-ui",
	"version": "0.1.0",
	"description": "Web UI for configuring and running Promptfoo LLM evaluations",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"eval:run": "tsx scripts/run-eval.ts",
		"eval:report": "tsx scripts/run-eval.ts report",
		"eval:reports": "tsx scripts/run-eval.ts reports"
	},
	"keywords": [
		"promptfoo",
		"llm",
		"evaluation",
		"testing",
		"ai"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"dotenv": "^16.0.0",
		"sveltekit-superforms": "^2.29.1",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-node": "^5.2.0",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/node": "^20.0.0",
		"autoprefixer": "^10.4.20",
		"daisyui": "^4.12.14",
		"postcss": "^8.4.47",
		"promptfoo": "^0.120.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^3.4.17",
		"tsx": "^4.0.0",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	}
}
